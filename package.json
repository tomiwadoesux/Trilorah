{
  "name": "ai-preacher-assistant",
  "private": true,
  "version": "0.0.1",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:frontend\" \"npm run dev:backend\" \"npm run dev:ml\"",
    "dev:frontend": "vite",
    "dev:backend": "wait-on tcp:5173 && cross-env NODE_ENV=development electron .",
    "dev:ml": "cd ml && ./venv/bin/python server.py",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@deepgram/sdk": "^4.11.3",
    "@ffmpeg/core": "^0.12.10",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@ffmpeg/util": "^0.12.2",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@xenova/transformers": "^2.17.2",
    "better-sqlite3": "^12.5.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "fabric": "^6.9.1",
    "ffmpeg-static": "^5.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.34.3",
    "html-to-image": "^1.11.13",
    "lucide-react": "^0.562.0",
    "obs-websocket-js": "^5.0.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.5.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "webmidi": "^3.1.14",
    "whisper-node": "^1.1.1",
    "ws": "^8.19.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/fabric": "^5.3.11",
    "@types/fluent-ffmpeg": "^2.1.28",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^9.0.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.1.0",
    "utf-8-validate": "^6.0.6"
  }
